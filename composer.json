{
    "name": "lamoda/tactician-queue-bundle",
    "description": "Symfony bundle for lamoda/tactician-queue library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lamoda developers",
            "homepage": "https://tech.lamoda.ru/"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "lamoda/queue-bundle": "^1.0",
        "lamoda/tactician-queue": "^1.0",
        "league/tactician": "^1.0",
        "symfony/serializer": "^3.4 || ^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "league/tactician-bundle": "^1.1",
        "phpunit/phpunit": "^7.5",
        "symfony/symfony": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\TacticianQueueBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\TacticianQueueBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
